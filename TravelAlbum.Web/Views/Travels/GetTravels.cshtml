@model TravelAlbum.Web.Models.TravelModels.TravelsOutputViewModel

@foreach (var travel in Model.travels)
{    
    <div class="clearfix travels-container">
        <div class="travel-title">@travel.Title</div>
        <img src="@String.Format("data:image/jpg;base64,{0}", travel.ImageData)" class="travels-image" />
        <p class="travels-description">@travel.Description</p>
    </div>
}




